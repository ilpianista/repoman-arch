#: repoman:182
msgid "A tool for manage your personal ArchLinux repository\\n"
msgstr "Uno strumento per gestire il vostro repository personale di ArchLinux\\n"

#: repoman:185
msgid "Action:\\n"
msgstr "Azione:\\n"

#: repoman:186
msgid "  -A, --add          add or update packages to repo\\n"
msgstr "  -A, --add          aggiunge o aggiorna i pacchetti del repo\\n"

#: repoman:187
msgid "  -R, --remove       remove packages from repo\\n"
msgstr "  -R, --remove       rimuove i pacchetti dal repo\\n"

#: repoman:188
msgid "      --create       create database repo, then add packages\\n"
msgstr "      --create       crea il database del repo e aggiunge i pacchetti\\n"

#: repoman:189
msgid "Options:\\n"
msgstr "Opzioni:\\n"

#: repoman:190
msgid "      --help         display this help and exit\\n"
msgstr "      --help         mostra questo help ed esce\\n"

#: repoman:191
msgid "      --verbose      print commands output\\n"
msgstr "      --verbose      visualizza l'output dei comandi\\n"

#: repoman:192
msgid "      --nocolor      do not use any color\\n"
msgstr "      --nocolor      non utilizza nessun colore\\n"

#: repoman:193
msgid "      --nochangelog  do not use a changelog file\\n"
msgstr "      --nochangelog  non utilizza un file di changelog\\n"

#: repoman:194
msgid "      --edit         ask for edit pkgbuild\\n"
msgstr "      --edit         chiede di editare il pkgbuild\\n"

#: repoman:195
msgid "      --clean        remove any files from workspace dir\\n"
msgstr "      --clean        rimuove tutti i file dalla directory di lavoro\\n"

#: repoman:196
msgid "      --noupload     do not upload any file to ftp\\n"
msgstr "      --noupload     non invia nessun file al ftp\\n"

#: repoman:197
msgid "      --local        use this for local repository\\n"
msgstr "      --local        usa questo come repository locale\\n"

#: repoman:198
msgid "      --nobuild      do not build packages if exist\\n"
msgstr "      --nobuild      se esiste, non crea i pacchetti pacchetti\\n"

#: repoman:222
msgid "REMOVED"
msgstr "RIMOSSO"

