msgid "I cannot get %s. Exiting."
msgstr "Nelze získat %s. Končím."

msgid "I cannot extract %s. Exiting."
msgstr "Nelze rozbalit %s. Končím."

msgid "%s: Makepkg fail. Exiting."
msgstr "%s: Makepkg selhalo. Končím."

msgid "Upload of %s fail. Exiting."
msgstr "Selhal upload %s. Končím."

msgid "skip, exist"
msgstr "přeskakuji, již existuje"

msgid "Updating:"
msgstr "Aktualizuji:"

msgid "added!"
msgstr "přidáno!"

msgid "updated!"
msgstr "aktualizováno!"

msgid "removed!"
msgstr "odstraněno!"

msgid "A tool to manage your personal ArchLinux repository\\n"
msgstr "Nástroj pro správu vlastního repozitáře pro ArchLinux\\n"

msgid "Usage: repoman action [OPTIONS] packages\\n"
msgstr "Použití: repoman akce [VOLBY] balíčky\\n"

msgid "Action:\\n"
msgstr "Akce:\\n"

msgid "  -A, --add          add or update packages to repo\\n"
msgstr "  -A, --add          přidat nebo aktualizovat balíčky v repozitáři\\n"

msgid "  -R, --remove       remove packages from repo\\n"
msgstr "  -R, --remove       odstranit balíčky z repozitáře\\n"

msgid "      --create       create database repo, then add packages\\n"
msgstr "      --create       vytvořit databázi repozitáře, poté přidat balíčky\\n"

msgid "Options:\\n"
msgstr "Volby:\\n"

msgid "      --help         display this help and exit\\n"
msgstr "      --help         vypsat tuto nápovědu a skončit\\n"

msgid "      --verbose      print commands output\\n"
msgstr "      --verbose      vypsat výstup příkazů\\n"

msgid "      --nocolor      do not use any color\\n"
msgstr "      --nocolor      nepoužívat barvy\\n"

msgid "      --nochangelog  do not use a changelog file\\n"
msgstr "      --nochangelog  nepoužívat soubor logování změn\\n"

msgid "      --edit         ask for edit pkgbuild\\n"
msgstr "      --edit         ptát se na úpravu pkgbuildu\\n"

msgid "      --clean        remove any files from workspace dir\\n"
msgstr "      --clean        odstranit jakékoliv soubory z pracovního adresáře\\n"

msgid "      --noupload     do not upload any file to ftp\\n"
msgstr "      --noupload     nepřenášet žádné soubory na ftp\\n"

msgid "      --local        use this for local repository\\n"
msgstr "      --local        použít toto pro lokální repozitář\\n"

msgid "      --nobuild      do not build packages if exist\\n"
msgstr "      --nobuild      nepřekládat balíčky, pokud již existují\\n"

msgid "REMOVED"
msgstr "ODSTRANĚNO"

msgid "Do you want to edit this PKGBUILD? (y/N):"
msgstr "Chcete upravit tento PKGBUILD? (a/N):"

msgid "y"
msgstr "a"

msgid "Downloading:"
msgstr "Stahuji:"

msgid "Packages: "
msgstr "Balíčky:"

msgid "Do you want to remove package build dir? (y/N):"
msgstr "Chcete odstranit sestavovací adresář balíčku? (a/N):"

msgid "Any files in workspace directory have been removed!\\n"
msgstr "Všechny soubory z pracovního adresáře byly odstraněny!\\n"

msgid "uploaded!\\n"
msgstr "přeneseno!\\n"

msgid "All copy of %s have been removed!\\n"
msgstr "Všechny kopie %s byly odstraněny!\\n"

msgid "This will update the whole repository!\\n"
msgstr "Toto provede aktualizaci celého repozitáře!\\n"

msgid "Extracting:"
msgstr "Rozbaluji:"

msgid "Building:"
msgstr "Sestavuji:"
